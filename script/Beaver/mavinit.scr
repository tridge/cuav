# example mavproxy startup script for Beaver on aircraft side
set streamrate 5
set moddebug 1
set heartbeat 0
module load cuav.modules.camera
camera set gcs_address 192.168.16.15
camera set quality 65
camera set gamma 950
camera set capbrightness 150
camera set bandwidth 30000
#camera boundary cuav/cuav/tests/CMAC-boundary.txt
camera set altitude 90
#camera start
#watch camera
